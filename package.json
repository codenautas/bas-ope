{
  "name": "operativos",
  "description": "Elementos base para operativos estad√≠sticos",
  "version": "0.7.13",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "contributors": [
    "Raquel N Salva <raquelzssalva@gmail.com>",
    "Eugenio Arosteguy <eugenio.arosteguy@gmail.com>",
    "Emilio Platzer <emilioplatzer@gmail.com>"
  ],
  "repository": "codenautas/operativos",
  "license": "MIT",
  "main": "dist/server/app-operativos.js",
  "types": "dist/server/app-operativos.d.ts",
  "files": [
    "dist",
    "install"
  ],
  "dependencies": {
    "backend-plus": "~0.31.5",
    "pg-triggers": "~0.2.3"
  },
  "devDependencies": {
    "types.d.ts": "~0.4.10",
    "mixin-patch": "~0.3.1"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p tsconfig-client.json && tsc -p tsconfig-server.json & mixin-patch",
    "build-force": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\")",
    "watch:buildS": "(tsc -p tsconfig-server.json --watch || echo \"continue w/error\")",
    "start": "node dist/server/server-operativos.js",
    "clink": "npm prune && npm i && npm run build",
    "git-commit": "git add package.json && (git commit -m \"upgrading version\" || true)",
    "publish-stack": "npm version patch && ncu -u && npm run git-commit && npm publish && git push"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "app",
    "ecmaVersion": 6
  }
}
